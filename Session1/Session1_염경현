      "source": [
        "country = df.groupby('Genre', as_index=False).sum() #장르를 기준으로 국가별로 판매량 더하기\r\n",
        "country = country.melt(id_vars = 'Genre', value_vars = ['NA_Sales', 'EU_Sales','JP_Sales','Other_Sales'])\r\n",
        "country['variable'] = country['variable'].str.replace('_Sales','')\r\n",
        "country = country.sort_values(by=['value'], axis=0, ascending=False, ignore_index=True)\r\n",
        "country = country.drop(country[(country['value']==0)| #판매량이 0인 행 삭제\r\n",
        "                     (country['Genre']=='Unknown')].index) #Genre의 결측치 제거\r\n",
        "\r\n",
        "plt.figure(figsize=(13,5))\r\n",
        "sns.set_style(\"whitegrid\")\r\n",
        "plt.title('Sales record: Country-Genre')\r\n",
        "sns.barplot(data=country, x=\"variable\", y=\"value\", hue=\"Genre\", ci =None)\r\n",
        "plt.ylabel('Sales record (thousands)')\r\n",
        "plt.xlabel('Country')\r\n",
        "plt.legend(loc='center left', bbox_to_anchor=(1, 0.5));"
